{
    "name": "knyga/generito",
    "type": "application",
    "description": "PHP generator based on distribution",
    "keywords": ["generator"],
    "homepage": "https://github.com/knyga/generito",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Knyga",
            "email": "oleksandrknyga@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">= 5.4",
        "rych/random": "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Generito\\": "." }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        }
    }
}